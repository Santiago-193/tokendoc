{
  "global": {
    "12": {
      "value": "$root * 1.5",
      "type": "sizing"
    },
    "16": {
      "value": "$root * 2",
      "type": "sizing"
    },
    "20": {
      "value": "$root * 2.5",
      "type": "sizing"
    },
    "24": {
      "value": "$root * 3",
      "type": "sizing"
    },
    "28": {
      "value": "$root * 3.5",
      "type": "sizing"
    },
    "30": {
      "value": "$root * 3.75",
      "type": "sizing"
    },
    "32": {
      "value": "$root * 4",
      "type": "sizing"
    },
    "40": {
      "value": "$root * 5",
      "type": "sizing"
    },
    "root": {
      "value": "8",
      "type": "other",
      "description": "valor root"
    },
    "theme": {
      "acent": {
        "100%": {
          "value": "#00C57B",
          "type": "color",
          "description": "Color para acentuar objetos interactivos"
        },
        "30%": {
          "value": "rgba({theme.font.primary} , 0.3)",
          "type": "color",
          "description": "Acento desactivado"
        }
      },
      "font": {
        "primary": {
          "value": "#495980",
          "type": "color",
          "description": "Para textos y otra información"
        },
        "60%": {
          "value": "rgba({theme.font.primary}, 0.6)",
          "type": "color",
          "description": "Para textos y otra información"
        }
      },
      "secondary": {
        "actions": {
          "value": "#91B2FF",
          "type": "color"
        }
      },
      "primary": {
        "bg": {
          "value": "#dee8ff",
          "type": "color",
          "description": "Color de fondo para las pantallas"
        }
      }
    },
    "font": {
      "primary": {
        "value": "Poppins",
        "type": "fontFamilies"
      }
    },
    "system": {
      "error": {
        "value": "#FE5353",
        "type": "color"
      },
      "warning": {
        "value": "#FECE53",
        "type": "color"
      }
    },
    "body": {
      "size": {
        "h3": {
          "value": "$16",
          "type": "fontSizes"
        },
        "h2": {
          "value": "$20",
          "type": "fontSizes"
        },
        "h1": {
          "value": "$40",
          "type": "fontSizes"
        },
        "info": {
          "value": "$12",
          "type": "fontSizes"
        },
        "body": {
          "value": "$root * 1.75",
          "type": "fontSizes"
        }
      },
      "h1": {
        "value": {
          "fontFamily": "$font.primary",
          "fontWeight": "$body.weigth.bold",
          "fontSize": "$hero.hero"
        },
        "type": "typography"
      },
      "h2": {
        "value": {
          "fontFamily": "$font.primary",
          "fontSize": "{body.size.h2}",
          "fontWeight": "$body.weigth.medium"
        },
        "type": "typography"
      },
      "h3": {
        "value": {
          "fontFamily": "$font.primary",
          "fontSize": "{body.size.h3}",
          "fontWeight": "{body.weigth.regular}"
        },
        "type": "typography"
      },
      "weigth": {
        "regular": {
          "value": "Regular",
          "type": "fontWeights"
        },
        "medium": {
          "value": "Medium",
          "type": "fontWeights"
        },
        "bold": {
          "value": "Bold",
          "type": "fontWeights"
        }
      },
      "body": {
        "value": {
          "fontFamily": "$font.primary",
          "fontWeight": "$body.weigth.regular",
          "fontSize": "$body.size.body"
        },
        "type": "typography"
      },
      "info": {
        "value": {
          "fontFamily": "$font.primary",
          "fontWeight": "$body.weigth.regular",
          "fontSize": "$body.size.info"
        },
        "type": "typography"
      }
    },
    "hero": {
      "h1": {
        "value": {
          "fontFamily": "$font.primary",
          "fontWeight": "$body.weigth.medium",
          "fontSize": "$body.size.h1"
        },
        "type": "typography"
      },
      "hero": {
        "value": "$28",
        "type": "fontSizes"
      }
    },
    "padding": {
      "xxs": {
        "value": "$root / 2",
        "type": "spacing"
      },
      "xsmall": {
        "value": "$root",
        "type": "spacing"
      },
      "small": {
        "value": "$12",
        "type": "spacing"
      },
      "medium": {
        "value": "$16",
        "type": "spacing"
      },
      "xl": {
        "value": "$40",
        "type": "spacing"
      },
      "l": {
        "value": "$20",
        "type": "spacing"
      }
    },
    "margin": {
      "xsmall": {
        "value": "$root",
        "type": "spacing"
      },
      "small": {
        "value": "$12",
        "type": "spacing"
      },
      "medium": {
        "value": "$16",
        "type": "spacing"
      },
      "xxl": {
        "value": "$40",
        "type": "spacing"
      },
      "xl": {
        "value": "$32",
        "type": "spacing"
      },
      "l": {
        "value": "$20",
        "type": "spacing"
      }
    },
    "border": {
      "value": "$root / 2",
      "type": "borderRadius"
    },
    "Cards": {
      "value": "$12",
      "type": "borderRadius"
    }
  },
  "Dark": {
    "theme": {
      "primary": {
        "100%": {
          "value": "#463f3f",
          "type": "color"
        }
      }
    }
  },
  "$themes": [],
  "$metadata": {
    "tokenSetOrder": [
      "global",
      "Dark"
    ]
  }
}